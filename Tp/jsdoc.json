{
    "source": {
      "include": ["./server.js","./server2.js","./server.test.js"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "tutorials":"./tutorials",
      "template":"custom-template"
    }
  }